{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "alwaysStrict": true,
      "baseUrl": "./src",
      "downlevelIteration": true,
      "esModuleInterop": true,
      "lib": ["esnext", "dom"],
      "module": "commonjs" ,
      "noEmit": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "strict": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "target": "ESNext",
  }
}
