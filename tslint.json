{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
      "semicolon": "always",
      "member-ordering": true,
      "ordered-imports": true,
      "object-literal-sort-keys": true,
      "trailing-comma": "always",
      "object-literal-key-quotes": true,
      "no-console": false,
      "interface-name": true
  },
  "rulesDirectory": []
}
